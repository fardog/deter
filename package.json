  "devDependencies": {
    "babel": "^5.8.21",
    "concat-stream": "^1.5.0",
    "eslint": "^1.2.1",
    "eslint-config-standard": "^4.1.0",
    "eslint-config-standard-react": "^1.0.4",
    "eslint-plugin-react": "^3.2.3",
    "eslint-plugin-standard": "^1.2.0",
    "standard": "^5.1.0",
    "tape": "^4.2.0"
